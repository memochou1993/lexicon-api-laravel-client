{
    "name": "memochou1993/lexicon-api-client-laravel",
    "description": "Lexicon API Client Library for Laravel",
    "type": "library",
    "keywords": [
        "localization"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Memo Chou",
            "email": "memochou1993@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "laravel/framework": "^8.0",
        "symfony/var-exporter": "^5.0",
        "guzzlehttp/guzzle": "^7.2",
        "memochou1993/lexicon-api-php-client": "^0.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "MemoChou1993\\Lexicon\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MemoChou1993\\Lexicon\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MemoChou1993\\Lexicon\\Providers\\LexiconServiceProvider"
            ],
            "aliases": {
                "Lexicon": "MemoChou1993\\Lexicon\\Facades\\Lexicon"
            }
        }
    }
}
