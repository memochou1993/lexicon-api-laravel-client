{
    "name": "memochou1993/localize-client-laravel",
    "description": "Localize API client for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Memo Chou",
            "email": "memochou1993@gmail.com"
        }
    ],
    "require": {
        "symfony/var-exporter": "^5.0",
        "guzzlehttp/guzzle": "^6.5",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "MemoChou\\Localize\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MemoChou\\Localize\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MemoChou\\Localize\\LocalizeServiceProvider"
            ],
            "aliases": {
                "Localize": "MemoChou\\Localize\\Facades\\Localize"
            }
        }
    }
}
