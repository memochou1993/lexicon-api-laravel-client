{
    "name": "memochou1993/lexicon-client-laravel",
    "description": "Lexicon API client for Laravel",
    "type": "library",
    "keywords": [
        "localization"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Memo Chou",
            "email": "memochou1993@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "laravel/framework": "^7.0",
        "symfony/var-exporter": "^5.0",
        "guzzlehttp/guzzle": "^6.5",
        "memochou1993/lexicon-client-php": "v0.1.7"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "MemoChou1993\\Lexicon\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MemoChou1993\\Lexicon\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MemoChou1993\\Lexicon\\Providers\\LexiconServiceProvider"
            ],
            "aliases": {
                "Lexicon": "MemoChou1993\\Lexicon\\Facades\\Lexicon"
            }
        }
    }
}
