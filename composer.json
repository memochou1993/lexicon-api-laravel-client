{
    "name": "memochou/localize-client-laravel",
    "description": "Localize API client for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Memo Chou",
            "email": "memochou1993@gmail.com"
        }
    ],
    "require": {
        "symfony/var-exporter": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "MemoChou\\Localize\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MemoChou\\Localize\\LocalizeServiceProvider"
            ],
            "aliases": {
                "Localize": "MemoChou\\Localize\\Facades\\Localize"
            }
        }
    }
}
